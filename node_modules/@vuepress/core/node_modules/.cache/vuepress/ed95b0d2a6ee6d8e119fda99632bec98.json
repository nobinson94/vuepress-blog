AAA,eADA,2BACA,MADA,EACA;AACA,WAAA,OAAA,IAAA,MAAA;;AACA,UAAA,KAAA,OAAA,GAAA,CAAA,EAAA;AACA,aAAA,OAAA,GAAA,CAAA;AACA,OAFA,MAEA,IACA,KAAA,OAAA,IAAA,IAAA,CAAA,KAAA,CAAA,KAAA,UAAA,GAAA,KAAA,WAAA,CADA,EAEA;AACA,aAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,UAAA,GAAA,KAAA,WAAA,IAAA,CAAA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,OAAA;AACA;AAXA,GApBA;AAiCA,EAAA,IAjCA,kBAiCA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,CADA;AAEA,MAAA,UAAA,EAAA,CAFA;AAGA,MAAA,WAAA,EAAA;AAHA,KAAA;AAKA;AAvCA,CAAA","sourcesContent":["<template>\n  <div class=\"posts\">\n    <div class=\"ui centered grid\">\n      <div class=\"sixteen wide column\">\n        <img class=\"ui centered rounded image\" src=\"../public/images/main_img.png\" style=\"width: 400px;\"/>\n      </div>\n      <div>\n        <p class=\"ui header\">Welcome to YT's first Dev Blog</p>\n        <p>by 권용태 (<a href=\"https://github.com/nobinson94\">@github:nobinson94</a>)</p>\n      </div>\n    </div>\n    <br>\n    <h4 class=\"ui horizontal divider header\">\n      <i class=\"tag icon\"></i>\n      Latest\n    </h4>\n    <div class=\"ui divided items\" v-for=\"post in postsForPage\" :key=\"post.path\">\n      <div class=\"item\">\n        <div class=\"content\">\n          <router-link :to=\"post.path\" class=\"header\">{{ post.frontmatter.title }}</router-link>\n          <div class=\"meta\">\n            <span>{{ post.frontmatter.date }}</span>\n          </div>\n          <div class=\"description\">\n            <p>{{ post.frontmatter.description }}</p>\n          </div>\n          <div class=\"extra\">\n            <div class=\"ui label\" v-for=\"tag in post.frontmatter.tags\">{{ tag }}</div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Vue from \"vue\";\nexport default {\n  props: ['page'],\n  computed: {\n    posts() {\n      let currentPage = this.page ? this.page : this.$page.path;\n      let posts = this.$site.pages\n        .filter(x => {\n          return x.path.match(new RegExp(`(${currentPage})(?=.*html)`));\n        })\n        .sort((a, b) => {\n          return new Date(b.frontmatter.date) - new Date(a.frontmatter.date);\n        });\n      this.postLength = posts.length;\n      return posts;\n    },\n    postsForPage() {\n      let postIndex = this.pageNum * this.postPerPage;\n      return this.posts.slice(postIndex, postIndex + this.postPerPage);\n    }\n  },\n  methods: {\n    increasePageNum(amount) {\n      this.pageNum += amount;\n      if (this.pageNum < 0) {\n        this.pageNum = 0;\n      } else if (\n        this.pageNum >= Math.round(this.postLength / this.postPerPage)\n      ) {\n        this.pageNum = Math.round(this.postLength / this.postPerPage) - 1;\n      }\n      console.log(this.pageNum);\n    }\n  },\n  data() {\n    return {\n      pageNum: 0,\n      postLength: 0,\n      postPerPage: 5\n    };\n  }\n};\n</script>\n\n<style>\n.post-list {\n\n}\n.post {\n  display: flex;\n}\n<style>\n"],"sourceRoot":"docs/.vuepress/components"}]}